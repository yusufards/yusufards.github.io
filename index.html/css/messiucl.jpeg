          var category		=$('.'+filter+'-'+i).attr("data-category");
          var action		=$('.'+filter+'-'+i).attr("data-action");
          var label			=$('.'+filter+'-'+i).attr('data-label');

						window.dataLayer.push({
                            event: even,
                            event_category: category,
                            event_action: action,
                            event_label: label,
                            data_source : 'Non AMP',
                        });

	  }
  </script>




